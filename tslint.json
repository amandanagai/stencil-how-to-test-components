{
  "extends": [
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "import-name": false,
    "variable-name": false,
    "function-name": false,
    "no-increment-decrement": false,
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ]
  }
}